{
  "name": "timberj",
  "version": "1.0.0",
  "description": "A lightweight, hierarchical logging library for Node.js with configurable log levels and tag-based organization",
  "main": "dist/TimberJ.js",
  "types": "dist/TimberJ.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm test",
    "examples": "cd examples && npm run all",
    "demo": "npm run build && node demo.js",
    "benchmark": "npm run build && node benchmark.js"
  },
  "keywords": [
    "logging",
    "logger",
    "debug",
    "console",
    "typescript",
    "nodejs",
    "hierarchical",
    "tagged"
  ],
  "author": "Kaito Kitaya",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kate941-su/TimberJ.git"
  },
  "bugs": {
    "url": "https://github.com/Kate941-su/TimberJ/issues"
  },
  "homepage": "https://github.com/Kate941-su/TimberJ#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
